package another_class;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.SQLException;



public class Intodb {
	static final String JDBC_DRIVER = "org.mariadb.jdbc.Driver";  // jdbc 드라이버 주소
	static final String DB_URL = "jdbc:mariadb://127.0.0.1:3306/test"; // DB 접속 주소
	static final String USERNAME = "root"; // DB ID
	static final String PASSWORD = "0000"; // DB Password
	static final String TABLENAME = "tb_rcept_num_test"; // DB Password
	
	
	
	public void insertdb() {
		
		Connection conn = null;
		PreparedStatement stmt = null;

			  try {
				
				String query = "INSERT INTO tb_rcept_num_test (corp_code, rcept_no) VALUES(?, ?);";
				
			
				
				Class.forName(JDBC_DRIVER);
				conn = DriverManager.getConnection(DB_URL,USERNAME,PASSWORD); 
				stmt = conn.prepareStatement(query);
				stmt.setString(1, "corp_code_01");				
				stmt.setString(2, "rcept_no_01");
			 
				stmt.executeUpdate();
				stmt.close();
			  } catch (SQLException e) {
				  System.out.println(e.getMessage ());
			  } catch (ClassNotFoundException e) {
				  System.out.println("ClassNotFoundException");
			  }
			  
	}
	public static void main(String[] args) {
		Intodb inin = new Intodb();
		inin.insertdb();
	}
}